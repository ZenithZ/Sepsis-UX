            patient['ATS'] = Number(patient['ATS']) > 3 ? 3 : patient['ATS'];
        }
        else {
            patient['ATS'] = patient['Overridden'];
            delete patient['Overridden'];
        }
        this.dataSource._updateChangeSubscription();
    }
    undoSeen(patient) {
