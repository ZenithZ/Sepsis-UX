=this.headerContainer.getScroll(),vertical=this.verticalScrollContainer.getScroll();switch(dir){case DragScrollDirection.LEFT:horizontal.scrollLeft-=48;break;case DragScrollDirection.RIGHT:horizontal.scrollLeft+=48;break;case DragScrollDirection.TOP:vertical.scrollTop-=48;break;case DragScrollDirection.BOTTOM:vertical.scrollTop+=48;break;case DragScrollDirection.BOTTOMLEFT:horizontal.scrollLeft-=48,vertical.scrollTop+=48;break;case DragScrollDirection.BOTTOMRIGHT:horizontal.scrollLeft+=48,vertical.scrollTop+=48;break;case DragScrollDirection.TOPLEFT:horizontal.scrollLeft-=48,vertical.scrollTop-=48;break;case DragScrollDirection.TOPRIGHT:horizontal.scrollLeft+=48,vertical.scrollTop-=48;break;default:return}this.wheelHandler()}isDefined(arg){return null!=arg}selectRange(arg){this.isDefined(arg)?(arg instanceof Array?arg.forEach(range=>this.setSelection(range)):this.setSelection(arg),this.notifyChanges()):this.clearCellSelection()}columnToVisibleIndex(field){return\"number\"==typeof field?field:this.visibleColumns.find(column=>column.field===field).visibleIndex}setSelection(range){const startNode={row:range.rowStart,column:this.columnToVisibleIndex(range.columnStart)},endNode={row:range.rowEnd,column:this.columnToVisibleIndex(range.columnEnd)};this.selectionService.pointerState.node=startNode,this.selectionService.selectRange(endNode,this.selectionService.pointerState),this.selectionService.addRangeMeta(endNode,this.selectionService.pointerState),this.selectionService.initPointerState()}getSelectedRanges(){return this.selectionService.ranges}extractDataFromSelection(source,formatters=!1,headers=!1){let columnsArray,record={};const selectedData=[],activeEl=this.selectionService.activeElement,selectionMap=Array.from(this.selectionService.selection).filter(tuple=>tuple[0]<source.length);this.cellSelection===GridSelectionMode.single&&activeEl&&selectionMap.push([activeEl.row,(new Set).add(activeEl.column)]);for(const[row,set]of selectionMap){if(!source[row])continue;const 