tyle(column){return column.dataType===DataType.Number}get_data(){const grid=this.grid;return grid.data?grid.data:[]}get_all_data(includeTransactions=!1){const grid=this.grid;return includeTransactions?grid.dataWithAddedInTransactionRows:grid.data?grid.data:[]}get_filtered_data(){return this.grid.filteredData}getSortStrategyPerColumn(fieldName){return this.get_column_by_name(fieldName)?this.get_column_by_name(fieldName).sortStrategy:void 0}addRowToData(rowData){const grid=this.grid;grid.transactions.enabled?grid.transactions.add({id:grid.primaryKey?rowData[grid.primaryKey]:rowData,type:TransactionType.ADD,newValue:rowData}):grid.data.push(rowData)}deleteRowFromData(rowID,index){const grid=this.grid;if(-1!==index)grid.transactions.enabled?grid.transactions.add({id:rowID,type:TransactionType.DELETE,newValue:null},grid.data[index]):grid.data.splice(index,1);else{const state=grid.transactions.getState(rowID);grid.transactions.add({id:rowID,type:TransactionType.DELETE,newValue:null},state&&state.recordRef)}}deleteRowById(rowId){let index;const grid=this.grid,data=this.get_all_data();index=grid.primaryKey?data.map(record=>record[grid.primaryKey]).indexOf(rowId):data.indexOf(rowId);const state=grid.transactions.getState(rowId);if(-1===index&&(!state||state.type===TransactionType.DELETE))return;grid.endEdit(!0),grid.onRowDeleted.emit({data:data[index]}),this.deleteRowFromData(rowId,index),grid.selectionService.isRowSelected(rowId)?grid.selectionService.deselectRow(rowId):grid.selectionService.clearHeaderCBState(),grid._pipeTrigger++,grid.notifyChanges();const dataAfterDelete=grid.transactions.enabled?grid.dataWithAddedInTransactionRows:data;grid.refreshSearch(),dataAfterDelete.length%grid.perPage==0&&dataAfterDelete.length/grid.perPage-1<grid.page&&0!==grid.page&&grid.page--}get_row_id(rowData){return this.grid.primaryKey?rowData[this.grid.primaryKey]:rowData}row_deleted_transaction(rowID){const grid=this.grid;if(!grid)return!1;if(!grid.transactions.enabled)return!1;const st